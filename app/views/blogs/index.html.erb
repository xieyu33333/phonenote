<div class="container_24">
<h3>博文列表</h3>
<%= link_to '发布文章', new_blog_path , :class=>"btn btn-primary goright"%>
<table class="table tabletop">
  <tr>
    <th>标题</th>
    <th>作者</th>
    <th>日期</th>
    <th></th>
    <th></th>
  </tr>

<% @blogs.each do |blog| %>
  <tr>
    
    <td><%= link_to blog.title , blog %></td>
    <td><%= blog.title %></td>
    <td><%= blog.created_at%></td>
    <td><%= link_to '编辑', edit_blog_path(blog) %></td>
    <td><%= link_to '删除', blog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
</div>