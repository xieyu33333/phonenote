<h1>Listing notes</h1>

<table>
  <tr>
    <th>Phone</th>
    <th>Email</th>
    <th>Qq</th>
    <th>Skype</th>
    <th>Name</th>
    <th>Job</th>
    <th>Tel</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notes.each do |note| %>
  <tr>
    <td><%= note.phone %></td>
    <td><%= note.email %></td>
    <td><%= note.qq %></td>
    <td><%= note.skype %></td>
    <td><%= note.name %></td>
    <td><%= note.job %></td>
    <td><%= note.tel %></td>
    <td><%= link_to 'Show', note %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Note', new_note_path %>
